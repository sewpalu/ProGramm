					<object class="wxBoxSizer">
						<orient>wxVERTICAL</orient>
						<object class="sizeritem">
							<option>0</option>
							<flag>wxALL|wxEXPAND</flag>
							<border>5</border>
							<object class="wxStaticText" name="m_staticText2">
								<label>Visualisation</label>
								<wrap>-1</wrap>
							</object>
						</object>
						<object class="sizeritem">
							<option>1</option>
							<flag>wxEXPAND | wxALL</flag>
							<border>5</border>
							<object class="wxNotebook" name="tabs">
								<object class="notebookpage">
									<label>CYK</label>
									<selected>1</selected>
									<object class="wxPanel" name="m_panel2">
										<style>wxTAB_TRAVERSAL</style>
										<object class="wxBoxSizer">
											<orient>wxVERTICAL</orient>
											<object class="sizeritem">
												<option>1</option>
												<flag>wxEXPAND | wxALL</flag>
												<border>5</border>
												<object class="wxPanel" name="m_panel9" subclass="CYKVisualisationTab">
													<style>wxTAB_TRAVERSAL</style>
												</object>
											</object>
										</object>
									</object>
								</object>
								<object class="notebookpage">
									<label>Ableitungen</label>
									<selected>0</selected>
									<object class="wxPanel" name="m_panel4">
										<style>wxTAB_TRAVERSAL</style>
										<object class="wxBoxSizer">
											<orient>wxVERTICAL</orient>
											<object class="sizeritem">
												<option>1</option>
												<flag>wxEXPAND | wxALL</flag>
												<border>5</border>
												<object class="wxPanel" name="m_panel10" subclass="STVisualisationTab">
													<style>wxTAB_TRAVERSAL</style>
												</object>
											</object>
										</object>
									</object>
								</object>
								<object class="notebookpage">
									<label>Grammatik Manager</label>
									<selected>0</selected>
									<object class="wxPanel" name="m_panel5">
										<style>wxTAB_TRAVERSAL</style>
										<object class="wxBoxSizer">
											<orient>wxVERTICAL</orient>
											<object class="sizeritem">
												<option>1</option>
												<flag>wxEXPAND | wxALL</flag>
												<border>5</border>
												<object class="wxPanel" name="m_panel11" subclass="ManagerTab">
													<style>wxTAB_TRAVERSAL</style>
												</object>
											</object>
										</object>
									</object>
								</object>
							</object>
						</object>
					</object>
				</object>
			</object>
		</object>
	</object>
	<object class="wxPanel" name="cyk_panel">
		<style>wxTAB_TRAVERSAL</style>
		<object class="wxBoxSizer">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxEXPAND | wxALL</flag>
				<border>5</border>
				<object class="wxPanel" name="cyk_steps" subclass="StepsDisplay">
